{
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["markdown"]
    }
  },
  "git.addedSign.hlGroup": "CocDiffAdd",
  "git.changedSign.hlGroup": "CocDiffChange",
  "git.removedSign.hlGroup": "CocDiffDelete",
  "git.topRemovedSign.hlGroup": "CocDiffDelete",
  "git.changeRemovedSign.hlGroup": "CocDiffDelete",

  "tsserver.enable": true,
  "eslint.enable": true,
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,

  "suggest.noselect": true,

  "dialog.floatHighlight": "CocFloating",
  "dialog.floatBorderHighlight": "CocFloating",

  "hover.floatConfig": {
    "border": true,
    "highlight": "CocHoverFloating",
    "winblend": 10
  },
  "signature.floatConfig": {
    "border": true,
    "highlight": "CocSignatureFloating",
    "winblend": 10
  },
  "suggest.floatConfig": {
    "border": true,
    "highlight": "CocSuggestFloating",
    "winblend": 10
  },
  "diagnostic.level": "hint",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.virtualTextPrefix": " â–¸ ",
  "diagnostic.errorSign": "\uf65b",
  "diagnostic.warningSign": "\uf525",
  "diagnostic.infoSign": "\uf7fb",
  "diagnostic.hintSign": "\uf7d6",
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck",
    "zsh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt",
    "zsh": "shfmt"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "highlight": "CocDiagnosticFloating",
    "winblend": 10
  },
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.autoformat": true
}
